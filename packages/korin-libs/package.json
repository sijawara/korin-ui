{
  "name": "@korinai/libs",
  "author": "Muhamad Rizki",
  "version": "1.0.3",
  "description": "Shared hooks, contexts, utils, and UI helpers for KorinAI apps",
  "private": false,
  "keywords": [
    "korinai",
    "hooks",
    "libs",
    "react"
  ],
  "license": "ISC",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./hooks/*": {
      "types": "./dist/index.d.ts",
      "import": "./dist/hooks/*.mjs",
      "require": "./dist/hooks/*.cjs",
      "default": "./dist/hooks/*.mjs"
    },
    "./ui/*": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui/*.mjs",
      "require": "./dist/ui/*.cjs",
      "default": "./dist/ui/*.mjs"
    },
    "./contexts/*": {
      "types": "./dist/index.d.ts",
      "import": "./dist/contexts/*.mjs",
      "require": "./dist/contexts/*.cjs",
      "default": "./dist/contexts/*.mjs"
    },
    "./types": "./dist/index.d.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "build": "vite build",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@ai-sdk/react": ">=2",
    "ai": ">=5",
    "react": ">=18",
    "react-dom": ">=18",
    "swr": ">=2"
  },
  "devDependencies": {
    "@monorepo/eslint-config": "workspace:*",
    "@monorepo/tsconfig": "workspace:*",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "fast-glob": "^3.3.3",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  }
}